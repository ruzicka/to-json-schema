    "test:coverage": "nyc mocha --recursive",
    "test:coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruzicka/to-json-schema.git"
  },
  "keywords": [
    "JSON",
    "schema",
    "JSON schema",
    "jsonschema",
    "converter",
    "convert"
